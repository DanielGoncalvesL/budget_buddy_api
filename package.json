{
  "name": "budget_buddy_api",
  "version": "0.0.0-development",
  "license": "MIT",
  "repository": "https://github.com/DanielGoncalvesL/budget_buddy_api",
  "author": "Daniel Gon√ßalves Lopes",
  "private": false,
  "scripts": {
    "start": "yarn build && node dist/main",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc -p tsconfig-build.json",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "destroy": "docker-compose down -v --rmi local",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "prepare": "is-ci || husky install",
    "test": "jest --passWithNoTests --runInBand --no-cache --silent",
    "test:unit": "yarn test --watch -c ./jest.unit.config.js",
    "test:integration": "yarn test --watch -c ./jest.integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:mutation": "stryker run",
    "test:watch": "yarn test --watch",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "apollo-server-express": "3.12.0",
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "jsonwebtoken": "9.0.0",
    "module-alias": "2.2.2",
    "mongodb": "5.3.0",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.29",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@jest-mock/express": "2.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "10.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@shelf/jest-mongodb": "4.1.7",
    "@stryker-mutator/core": "6.4.2",
    "@stryker-mutator/jest-runner": "6.4.2",
    "@stryker-mutator/typescript-checker": "6.4.2",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/graphql": "14.5.0",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "9.0.1",
    "@types/module-alias": "2.0.1",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.1",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "commitizen": "4.3.0",
    "coveralls": "3.1.1",
    "eslint": "8.38.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "jest-mock": "29.5.0",
    "jest-mock-extended": "3.0.4",
    "lint-staged": "13.2.1",
    "pg-mem": "2.3.5",
    "prettier": "2.8.7",
    "rimraf": "5.0.0",
    "semantic-release": "21.0.1",
    "semantic-release-docker": "2.2.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4",
    "uuid": "9.0.0"
  }
}
